<%= form_with url: '/static_pages', method: 'get', local: true do |f| %>
    <%= f.label :user_id, 'Flickr ID: ' %>
    <%= f.text_field :user_id %>
    <%= f.submit 'Submit' %><br>
<% end %>
<% if params[:user_id].present? %>
    <% @photo_urls.each do |url| %>
        <%= image_tag(url) %><br>
    <% end %>
<% end %>
